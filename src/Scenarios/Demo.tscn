script/source = "extends Node2D

onready var goals: Node2D = $Goals
onready var accept_dialog := $AcceptDialog as AcceptDialog
var ass: bool setget set_ass

func set_ass(value):
	ass = value
	ProjectSettings.set_setting(\"gui/theme/custom\", \"res://assets/default_theme.tres\")
	propagate_call(\"update\")
	accept_dialog.update()

func _ready() -> void:
	
	for goal in goals.get_children():
		
		if (goal as Node2D).connect(\"occupation_shifted\", self, \"_on_Goal_occupation_shifted\") != OK:
			push_error(\"%s wouldn't able to connect to %s\" % [self, goal])


func _on_Goal_occupation_shifted(was_occupied: bool) -> void:
	
	if not was_occupied:
		return
	
	var goals_count := goals.get_child_count()
	
	for goal in goals.get_children():
		
		if goal.is_occupied:
			goals_count -= 1
	
	if goals_count == 0:
		accept_dialog.popup()


func _on_AcceptDialog_confirmed() -> void:
	#UI.reload()
	push_warning(\"TODO: Add UI reload.\") # TODO
"

